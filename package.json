{
  "name": "narasi-player",
  "description": "Narasi Player for Streaming VOD & Live",
  "version": "1.0.3",
  "keywords": [
    "player",
    "narasi tv",
    "live streaming",
    "vod",
    "videojs",
    "video vast",
    "ads"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name narasi-player ./src/index.js"
  },
  "main": "./dist/narasi-player.common.js",
  "repository": "https://github.com/narasi-products/narasi-player",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "plugins/*"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "remove": "^0.1.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.14",
    "@vue/cli-plugin-eslint": "~4.5.14",
    "@vue/cli-service": "~4.5.14",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "video.js": "^7.15.4",
    "videojs-contrib-quality-levels": "^2.1.0",
    "videojs-hls-quality-selector": "^1.1.4",
    "videojs-vast-vpaid": "^3.0.0-beta.8",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
